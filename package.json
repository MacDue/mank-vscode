{
    "name": "mank-lang",
    "displayName": "Mank Lang",
    "description": "Mank language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mank",
            "aliases": ["Mank", "mank"],
            "extensions": [".mank"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mank",
            "scopeName": "source.mank",
            "path": "./syntaxes/mank.tmLanguage.json"
        }]
    }
}